{
    "name": "Create T3 App UnOfficial",
    "description": "The best way to start a full-stack, typesafe Next.js app",
    "categories": [
        "NextJS",
        "t3",
        "Full-Stack",
        "t3-stack"
    ],
    "icon": "https://create.t3.gg/images/t3-dark.svg",
    "website": "https://create.t3.gg/",
    "publisher": "Sparsh Kaushik",
    "params": [
        {
            "id": "language",
            "name": "Language",
            "type": "enum",
            "default": "ts",
            "options": {
                "js": "Javascript",
                "ts": "Typescript"
            },
            "required": true
        },
        {
            "id": "tailwind",
            "name": "Use Tailwind ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "trpc",
            "name": "Use tRPC ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "nextauth",       
            "name": "Use NextAuth ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "primsa",
            "name": "Use Prisma ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "drizzle",
            "name": "Use Drizzle ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "approuter",
            "name": "Use App Router ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "git_init",
            "name": "Initialize New GIT Repo ?",
            "type": "boolean",
            "default": true,
            "required": true
        },
        {
            "id": "db",
            "name": "Database",
            "type": "enum",
            "default": "none",
            "options": {
                "none": "None",
                "mysql": "MySQL",
                "postgres": "Postgres",
                "sqlite": "SQLite",
                "planetscale": "PlanetScale"
            },
            "required": true
        }
    ]
}